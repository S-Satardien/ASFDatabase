<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net472</TargetFramework>
    <RootNamespace>ASFDatabase</RootNamespace>
    <AssemblyName>ASFDatabase</AssemblyName>
    <OutputPath>bin\</OutputPath>
    <EnableDefaultCompileItems>false</EnableDefaultCompileItems>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CodeDom.Providers.DotNetCompilerPlatform" Version="3.6.0" />
    <PackageReference Include="Microsoft.Net.Compilers" Version="3.6.0" />
  </ItemGroup>

  <ItemGroup>
    <!-- Add references to your web forms files -->
    <Content Include="**\*.aspx" />
    <Content Include="**\*.aspx.cs" />
    <Content Include="**\*.aspx.designer.cs" />
    <Content Include="**\*.ascx" />
    <Content Include="**\*.ascx.cs" />
    <Content Include="**\*.ascx.designer.cs" />
    <Content Include="**\*.css" />
    <Content Include="**\*.js" />
    <Content Include="**\*.config" />
    <Content Include="Web.config" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="**\*.cs" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="App_Data\" />
    <Folder Include="App_Code\" />
  </ItemGroup>

</Project>
